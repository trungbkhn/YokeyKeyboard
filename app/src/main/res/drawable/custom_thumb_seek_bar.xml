<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <size
                android:width="20dp"
                android:height="20dp" />
            <gradient
                android:centerColor="#FF35FD"
                android:endColor="#FF8E61"
                android:startColor="#4355FF" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:right="2dp"
        android:left="2dp"
        android:top="2dp">
        <shape android:shape="oval">
            <solid android:color="#FFFDF6" />
            <size
                android:width="20dp"
                android:height="20dp" />
        </shape>
    </item>
</layer-list>